{
  "name": "@empoleon/solid-floating-ui",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "solid": "./dist/source/index.js",
      "node": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "browser": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "rollup -c --no-sourcemap",
    "build:prod": "rollup -c --no-sourcemap",
    "prepublishOnly": "npm run build:prod",
    "test": "vitest",
    "lint": "biome lint",
    "newchangeset": "yarn exec changeset",
    "version:dev": "yarn version prerelease",
    "release:dev": "yarn npm publish --tag dev"
  },
  "sideEffects": false,
  "keywords": [
    "solid",
    "solidjs",
    "solid-js",
    "positioning",
    "tooltip",
    "floating-ui"
  ],
  "author": "Paul M",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulm17/floating-ui-solid"
  },
  "license": "MIT",
  "description": "SolidJS bindings for Floating UI",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "^2.29.5",
    "@changesets/get-release-plan": "3.0.17",
    "@changesets/types": "5.2.1",
    "@eslint/js": "^9.23.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "eslint": "^9.23.0",
    "eslint-plugin-solid": "^0.14.5",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "rollup": "^4.37.0",
    "rollup-plugin-summary": "^3.0.1",
    "rollup-preset-solid": "^3.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-solid": "^2.11.6",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "@floating-ui/dom": "1.7.1",
    "@floating-ui/utils": "0.2.8"
  },
  "browser": {},
  "typesVersions": {},
  "packageManager": "yarn@4.9.1"
}
